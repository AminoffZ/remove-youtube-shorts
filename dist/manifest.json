{
  "manifest_version": 3,
  "name": "Remove YouTube Shorts",
  "version": "0.1.0",
  "web_accessible_resources": [
    {
      "resources": ["script.js"],
      "matches": ["https://*.youtube.com/*"]
    }
  ],
  "content_scripts": [
    {
      "matches": ["https://*.youtube.com/*"],
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_icon": "./images/icon256.png"
  }
}
